{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "font-synthesis"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ]
  }
}